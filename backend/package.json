{
  "name": "zeta-backend",
  "version": "2.0.0",
  "description": "Zeta Science Platform Backend - Industry Grade",
  "main": "server.js",
  "scripts": {
  "start": "node server.js",
  "dev": "nodemon server.js",
  "test": "jest --coverage --detectOpenHandles --forceExit",
  "test:watch": "jest --watch",
  "test:unit": "jest tests/unit --coverage",
  "test:integration": "jest tests/integration --coverage",
  "test:e2e": "jest tests/e2e",
  "lint": "eslint .",
  "lint:fix": "eslint . --fix",
  "format": "prettier --write \"**/*.{js,json,md}\"",
  "validate": "npm run lint && npm run test",
  "vercel-build": "echo 'Building for Vercel'",
  "seed": "node scripts/seed.js",
  "migrate": "node scripts/migrate.js"
},
  "engines": {
    "node": "24.x"
  },
  "keywords": ["science", "education", "platform"],
  "author": "Zeta Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-mongo-sanitize": "^2.2.0",
    "xss-clean": "^0.1.4",
    "hpp": "^0.2.3",
    "joi": "^17.11.0",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "winston": "^3.11.0",
    "nodemailer": "^6.9.7",
    "sharp": "^0.33.1",
    "socket.io": "^4.6.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^9.1.3",
    "@types/jest": "^29.5.11",
    "eslint": "^8.56.0"
  }
}